{
  "name": "docusaurus-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference-docs": "cd content-repo && PIPENV_IGNORE_VIRTUALENVS=1  ./create-docs.sh",
    "test": "cd content-repo && PIPENV_IGNORE_VIRTUALENVS=1 pipenv install --dev && PIPENV_IGNORE_VIRTUALENVS=1 pipenv run pytest -v",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npm test && npm run reference-docs && copyfiles -E -V -f docs/doc_imgs/* static/doc_imgs/. && replace 'doc_imgs' '/doc_imgs' docs/* --include='*.md*' && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.37",
    "@docusaurus/mdx-loader": "^2.0.0-alpha.37",
    "@docusaurus/plugin-content-blog": "^2.0.0-alpha.37",
    "@docusaurus/plugin-content-docs": "^2.0.0-alpha.37",
    "@docusaurus/preset-classic": "^2.0.0-alpha.37",
    "@docusaurus/theme-live-codeblock": "^2.0.0-alpha.37",
    "@docusaurus/theme-search-algolia": "^2.0.0-alpha.37",
    "classnames": "^2.2.6",
    "copyfiles": "^2.1.1",
    "js-yaml": "^3.13.1",
    "netlify-cms-app": "^2.9.8-beta.4",
    "node-sass": "^4.13.0",
    "panw-blox": "^1.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-particles-js": "^2.7.0",
    "react-scroll-up-button": "^1.6.4",
    "replace": "^1.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
